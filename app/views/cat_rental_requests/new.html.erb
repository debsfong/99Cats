<h1> Cat Renter Request Form </h1>
<form action="<%= cat_rental_requests_url%>" method="post">
  <label> Name:
    <select name="cat_rental_request[cat_id]">
      <% @cats.each do |cat| %>
      <option name="cat_rental_request[cat_id]" value="cat.id"> <%= cat.name %></option>
      <% end %>
    </select>
  </label>
  <br>

  <label> Start Date:
    <input type=date name="cat_rental_request[start_date]">
  </label>
  <br>

  <label> End Date:
    <input type=date name="cat_rental_request[end_date]">
  </label>
  <br>

  <input type="submit" value="Create Request">
</form>
